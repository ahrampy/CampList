{
  "name": "camp_list",
  "version": "1.0.0",
  "description": "Find and share camp spots on blm land",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "server": "nodemon app.js --ignore './frontend/'",
    "frontend-install": "npm install --prefix frontend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run frontend\"",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahrampy/blmFriends.git"
  },
  "keywords": [
    "camping",
    "blm",
    "california"
  ],
  "author": "max li, adrian rampy, will smith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ahrampy/CampList/issues"
  },
  "homepage": "https://github.com/ahrampy/CampList#readme",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "concurrently": "^5.0.1",
    "express": "^4.17.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.8.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "validator": "^12.1.0"
  },
  "devDependencies": {
    "dotenv": "^8.2.0",
    "nodemon": "^1.19.4"
  }
}
